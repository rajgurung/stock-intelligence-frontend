{
  "ci": {
    "collect": {
      "url": [
        "https://stock-intelligence-frontend.vercel.app",
        "https://stock-intelligence-frontend.vercel.app/#markets",
        "https://stock-intelligence-frontend.vercel.app/#watchlist",
        "https://stock-intelligence-frontend.vercel.app/#comparison"
      ],
      "numberOfRuns": 3,
      "settings": {
        "chromeFlags": "--no-sandbox --disable-dev-shm-usage",
        "preset": "desktop"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", {"minScore": 0.8}],
        "categories:accessibility": ["error", {"minScore": 0.9}],
        "categories:best-practices": ["warn", {"minScore": 0.9}],
        "categories:seo": ["warn", {"minScore": 0.9}],
        "categories:pwa": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}